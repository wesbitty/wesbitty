{
  "name": "ui",
  "version": "0.0.4",
  "description": "Wesbitty UI Component library",
  "keywords": [
    "storybook",
    "ui",
    "components",
    "wesbitty"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "source": "src/index.tsx",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "node --experimental-modules src/lib/Icon/generate.js",
    "build": "rollup -c",
    "build-storybook": "storybook build",
    "gen:doc": "tsc --declaration --outDir docs --declarationMap",
    "build:tailwind": "tailwindcss build -i src/theme/main.css -o src/theme/global.css -c tailwind.config.js",
    "storybook": "storybook dev -p 6006",
    "clean": "bash -c 'rm -rf lib docs storybook-static'",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!react-feather)"
    ]
  },
  "resolutions": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
