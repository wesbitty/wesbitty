{
  "name": "ui",
  "version": "0.0.4",
  "description": "Wesbitty UI Libary",
  "repository": {
    "type": "git",
    "url": "https://github.com/wesbitty/wesbitty.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "author": {
    "name": "iamNicholax <Wesbitty Inc>",
    "email": "wesbitty@outlook.com"
  },
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "npx svgr --out-dir src/images src/images",
    "build": "babel src -d lib --extensions \".js,.jsx,.ts,.tsx\" --ignore \"**/*.stories.tsx\" --ignore \"**/*.stories.js\"",
    "postbuild": "storybook build",
    "types": "tsc --declaration --emitDeclarationOnly --outDir lib --declarationMap",
    "types-check": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "lint": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "clean": "rm -rf lib storybook-static",
    "release": "dotenv npm run build & npm run types && auto shipit"
  },
  "dependencies": {
    "@emotion/weak-memoize": "^0.3.0",
    "@storybook/theming": "^7.0.2",
    "@types/pluralize": "^0.0.29",
    "@types/prismjs": "^1.16.6",
    "@types/react-modal": "^3.12.1",
    "@types/uuid": "^8.3.1",
    "copy-to-clipboard": "^3.3.1",
    "human-format": "^0.11.0",
    "pluralize": "^8.0.0",
    "polished": "^3.6.4",
    "prismjs": "1.25.0",
    "react-github-button": "^0.1.11",
    "react-modal": "^3.11.2",
    "react-popper-tooltip": "^2.11.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@emotion/babel-plugin": "^11.10.6",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-docs": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-storysource": "^7.0.2",
    "@storybook/eslint-config-storybook": "^3.1.2",
    "@storybook/linter-config": "^3.1.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@svgr/cli": "^6.1.1",
    "@types/fs-extra": "^9.0.1",
    "auto": "^9.50.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.0.0",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-plugin-storybook": "^0.6.11",
    "node-fetch": "^2.6.0",
    "prettier": "^2.4.0",
    "react": "17",
    "react-dom": "17",
    "seedrandom": "^3.0.5",
    "storybook": "^7.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  }
}
