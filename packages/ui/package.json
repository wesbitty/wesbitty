{
  "name": "ui",
  "version": "0.0.23",
  "description": "Wesbitty UI Component library",
  "keywords": [
    "components",
    "library",
    "storybook",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wesbitty/wesbitty.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib/index.js",
  "files": [
    "lib",
    "config.js",
    "colors.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib && tsc",
    "build-storybook": "storybook build",
    "build:css": "npx tailwindcss -i ./styles/input.css -o styles/output.css -c ./tailwind.config.js",
    "build:icons": "node --experimental-modules scripts/generate-icons.cjs",
    "clean": "rimraf lib storybook-static",
    "release": "release-it",
    "storybook": "storybook dev -s ./src/static -p 6006"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
