{
  "name": "@wesbitty/ui",
  "version": "0.0.3",
  "description": "Wesbitty UI Component library",
  "keywords": [
    "storybook",
    "ui",
    "components",
    "library",
    "wesbitty"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "source": "src/index.tsx",
