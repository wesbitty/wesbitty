{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true
  },
  "exclude": ["node_modules"],
  "include": [
    "process.d.ts",
    "next-env.d.ts",
    "next-auth.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/protected-page.jsx"
, "@wesbitty/analytics/GoogleAnalytics.ts", "@wesbitty/analytics/Plausible.js" , "@wesbitty/analytics/GoogleAnalytics.js", "@wesbitty/analytics/SimpleAnalytics.js" ]
}
