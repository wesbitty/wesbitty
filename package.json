{
  "name": "wesbitty",
  "description": "We've Got Your Wishlist",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "workspaces": [
    "apps/web",
    "packages/*",
    "examples/*",
    "tests"
  ],
  "author": "Wesbitty, Inc.",
  "scripts": {
    "build": "turbo run build",
    "build:www": "turbo run build --filter=web",
    "dev": "turbo run dev --filter=web",
    "format:prettier": "prettier -w .",
    "rm": "npm run rm:examples && npm run rm:apps",
    "rm:examples": "bash -c 'rm -rf examples/*/.turbo examples/*/.wesjet examples/*/.next packages/*/dist'",
    "rm:apps": "bash -c 'rm -rf apps/*/.wesjet apps/*/.next  apps/*/.turbo'",
    "test": "jest"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.0",
    "semver": "^7.3.8",
    "simple-git-hooks": "^2.8.1",
    "turbo": "^1.6.3"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "babel-jest": "^29.3.1",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1"
  }
}
