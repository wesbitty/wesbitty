{
  "name": "wesbitty",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "workspaces": [
    "apps/www",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "bash -c 'rm -rf examples/*/.wesjet examples/*/.next apps/*/.wesjet apps/*/.next packages/*/dist'",
    "format:prettier": "prettier -w .",
    "dev": "npm run dev -w=www"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lerna": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.0",
    "semver": "^7.3.8",
    "simple-git-hooks": "^2.8.1"
  }
}
