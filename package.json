{
  "name": "wesbitty",
  "description": "We've Got Your Wishlist",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "workspaces": [
    "apps/web",
    "packages/*",
    "examples/*",
    "tests"
  ],
  "author": "Wesbitty, Inc.",
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build:app": "npm run build -w=./apps/web",
    "build:examples": "npm run build -w=./examples",
    "build:packages": "npm run build -w=./packages",
    "dev": "npm run dev -w=./apps/web",
    "prettier": "prettier --write \"**/*.{css,ts,tsx,md,mdx}\"",
    "clean": "bash -c 'rm -rf examples/*/.wesjet examples/*/.next packages/*/dist .vercel apps/*/.wesjet apps/*/.next'",
    "test": "jest"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.1",
    "mini-svg-data-uri": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.16.6",
    "@types/jest": "^27.0.3",
    "axios": "^0.24.0",
    "babel-jest": "^27.3.1",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "tsx": "^3.7.1",
    "typescript": "^4.5.2"
  }
}
